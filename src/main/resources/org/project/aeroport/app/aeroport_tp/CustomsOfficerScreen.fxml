<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.project.aeroport.app.aeroport_tp.controller.worker.CustomsOfficerController">
    <Label text="Проверка багажа" />
    <TextField fx:id="baggageIdField" promptText="Введите номер паспорта" />
    <ChoiceBox fx:id="inspectionResultBox" />
    <Button text="Проверить" onAction="#inspectBaggage" />
    <Button text="Назад" onAction="#handleBack" />
</VBox>
