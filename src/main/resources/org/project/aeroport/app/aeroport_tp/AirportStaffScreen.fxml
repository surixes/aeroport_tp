<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.project.aeroport.app.aeroport_tp.controller.worker.AirportStaffController">
    <Label text="Проверка билета" />
    <TextField fx:id="passportField" promptText="Номер паспорта" />
    <TextField fx:id="ticketField" promptText="Номер билета" />
    <Button text="Проверить паспорт и билет" onAction="#verifyPassportAndTicket" />
    <Button text="Назад" onAction="#handleBack"/>

</VBox>
